@model PersonnelTrainingViewModel
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>
<div class="container-fluid">
    <form asp-action="Edit" method="post" enctype="multipart/form-data">
        <input type="hidden" asp-for="PersonnelTrainingId" />
        <div class="form-group">
            <label asp-for="OrganizationId"></label>
            <select asp-for="OrganizationId" class="form-control" asp-items="ViewBag.Organizations"></select>
        </div>
        <div class="form-group">
            <label asp-for="LenOfSch"></label>
            <input type="text" class="form-control" asp-for="LenOfSch">
        </div>
        <div class="form-group">
            <label asp-for="Year"></label>
            <input type="text" class="form-control" asp-for="Year">
        </div>
        <div class="form-group">
            <label asp-for="CulProPath"></label>
            <input type="file" asp-for="CulProPath">
        </div>
        <div class="form-group">
            <label asp-for="MeeSumPath"></label>
            <input type="file" asp-for="MeeSumPath">
        </div>
        <div class="form-group">
            <label asp-for="Status"></label>
            <select asp-for="Status" class="form-control" asp-items="ViewBag.Statuss"></select>
            @*@Html.DropDownListFor(m=>m.Status,ViewBag.Statuss as IEnumerable<SelectListItem>, new { @class= "form-control" })*@
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-default btn-danger pull-right">保存</button>
        </div>
    </form>
</div>

